                            </children>
                        </system>
                    </children>
                </admin>
            </resources>
        </acl>
        <translate>
            <modules>
                <Flint_Feefo>
                    <files>
                        <default>Flint_Feefo.csv</default>
                    </files>
                </Flint_Feefo>
            </modules>
        </translate>
    </adminhtml>
