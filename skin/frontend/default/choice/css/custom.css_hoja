body          {  font-family: 'Open Sans', sans-serif; font-size:14px; line-height:18px; color:#696969; min-width:320px; background:url(../images/body-bg.jpg) 0 0 repeat-x #fff; position:relative; -webkit-text-size-adjust: none; }
body > .wrapper { position:relative;}
.wrapper-tail{}

img           { border:0; vertical-align:top; }

:focus        { outline:0; }


.page-title h1,
.page-title h2 {
	font-family: 'Open Sans', sans-serif;
	font-size:25px;
	line-height:30px;
	color:#212121;
	font-weight: 700;
}
.padding-s a{}
/* Layout ================================================================================ */
.wrapper {width:100%; overflow:hidden;}
.tail-top {  }
.page { margin:0 auto; 	padding:0 0 0; 	text-align:left; }
.page-print { background:#fff; padding:20px; text-align:left; }
.page-empty { background:#fff; padding:20px; text-align:left; }
.page-popup { padding:20px; text-align:left; margin:0 auto; background:#fff;}
.page-popup  .ratings .rating-box {float:none; margin:0 auto 10px;}

.featured-image-left{float: left; margin-right: 10px;}

/* Base Columns */
.col-left { float:left; }
.col-main { float:left; }
.col-right { float:right; }

/* 1 Column Layout */
.col1-layout .col-main {float:none;clear: both; }

/* 2 Columns Layout */
.col2-left-layout .col-main { float:right; }
.col2-right-layout .col-main { }

/* 3 Columns Layout */
.col3-layout .col-main {  }
.col3-layout .col-wrapper { float:left; }
.col3-layout .col-wrapper .col-main { float:right; }

/* Content Columns */
.col2-set .col-1 {  }
.col2-set .col-2 {  }
.col2-set .col-narrow { width:33%; }
.col2-set .col-wide { width:65%; }
.cart-collaterals .col2-set .col-1,
.cart-collaterals .col2-set .col-2 { width:48%; float:left;}
.cart-collaterals .col2-set .col-1 {margin-right:20px;}
.grid_custom_1 {width:65%; float:left;}
.grid_custom_2 {width:34%; float:right;}

.col3-set .col-1 { float:left; width:32%; }
.col3-set .col-2 { float:left; width:32%; margin-left:2%; }
.col3-set .col-3 { float:right; width:32%; }

.col4-set .col-1 { float:left; width:23.5%; }
.col4-set .col-2 { float:left; width:23.5%; margin:0 2%; }
.col4-set .col-3 { float:left; width:23.5%; }
.col4-set .col-4 { float:right; width:23.5%; }

.col5-set .col-1 { float:left; width:17.5%; border:1px solid #DCDCDC; margin-bottom:15px; }
.col5-set .col-2 { float:left; width:17.5%; margin:0 1.5%;  border:1px solid #DCDCDC; margin-bottom:15px; }
.col5-set .col-3 { float:left; width:17.5%;  border:1px solid #DCDCDC; margin-bottom:15px;}
.col5-set .col-4 { float:left; width:17.5%; margin:0 1.5%;  border:1px solid #DCDCDC; margin-bottom:15px;}
.col5-set .col-5 { float:left; width:17.5%;  border:1px solid #DCDCDC; margin-bottom:15px;}

.std .image_right{
	float:right;
	padding: 0 0 10px 10px;
}
.std .image_left{
	float:left;
	padding: 0 10px 10px 0px;
}
.special-offer-container .special-offer-outer .product-list-box .product-box .add-btn .add-to-basket{color:#fff !important}
.special-offer-container .special-offer-outer .product-list-box .product-box .add-btn .cart-img{padding-top:6px}
.special-offer-container a:hover .add-btn {color:#072c73 !important}


.onestepcheckout-column-left ul{list-style:none;}
.onestepcheckout-column-middle label{display:initial}
.std a{color:#337ab7 !important,text-decoration:none}
.cart-img{padding-top:7px;  padding-top: 7px;cursor: pointer;}
.btn-update{margin-bottom:5px}
.btn-clear{padding:7px 9px}
.my-basket-container .page-title{margin-top:34px}
#custommenu .act .parentMenu a{color:red}
.product-slider .page-title{margin-top:0}
