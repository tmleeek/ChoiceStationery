.toner-configurator select{
    padding:4px 0 3px 25px;
    margin: 0;
    -webkit-border-radius:4px;
    -moz-border-radius:4px;
    border-radius:1px;
    background: #f8f8f8;
    color:#888;
    border:none;
    outline:none;
    display: inline-block;
    -moz-appearance:none;
    appearance:none;
    cursor:pointer;
    height:30px;
    font-size: 13px;
    width:198px;
}
.navigation{display:none;}
.product-description-detail{text-align:left;line-height:28px;font-size:14px}
.tab-content{margin-top: 10px;
text-align: left;
line-height: 25px;
font-size:14px}
.col1-layout .product-shop{font-size: 14px;text-align: left;}
.col1-layout .add-to-box .bottom .add-btn-cart{position: absolute;
  bottom: 11px;
  right: 15px;
  width: 60%;}
#onestepcheckout-form .group-select{text-align:left}
.onestepcheckout-column-left ul {
  list-style: none;
  margin: 0px;
  padding: 0;
}

.onestepcheckout-coupons {
  float: right;
  margin-bottom: 15px;
}

.onestepcheckout-coupons button {
  float: right;
  margin-top: 10px;
}
.onestepcheckout-place-order-wrapper button {
  float: right;
}

#menu-button {
	background-color: #FFF !important;
}
#menu-button > a >span:first-child {
  display: none;
}
#menu-button > a {
	position: relative !important;
	float: right !important;
	padding: 9px 11px !important;
	margin-top: 8px;
	margin-right: 15px;
	margin-bottom: 8px;
	background-color: transparent;
	background-image: none;
	border: 1px solid transparent;
	border-radius: 4px;
	width: 43px !important;
	border-color: #8DF;
}
#menu-button a:after {
  content: "ïƒ‰";
  font-family: fontawesome;
  font-size: 24px;
  color: #888;
  font-weight: lighter;
  float: right;
}
div#menu-button {
  padding: 0px;
}


